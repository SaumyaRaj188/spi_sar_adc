TD_tb_spi_adc.adc_hard_reset ;
    %load/vec4 v0x58481d50afb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x58481d4b1230_0, 0, 2;
    %fork TD_tb_spi_adc.spi_read, S_0x58481d502e80;
    %join;
    %delay 200000, 0;
    %load/vec4 v0x58481d50afb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
T_0.4 ;
    %load/vec4 v0x58481d50afb0_0;
    %nor/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.5, 6;
    %wait E_0x58481d490190;
    %jmp T_0.4;
T_0.5 ;
T_0.2 ;
T_0.0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x58481d4e2400_0, 0, 2;
