T_3.14 ; Top of for-loop
    %load/vec4 v0x58481d4e52f0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_3.15, 5;
    %load/vec4 v0x58481d4c2f90_0;
