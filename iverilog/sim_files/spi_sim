TD_tb_spi_adc.adc_hard_reset ;
    %load/vec4 v0x564845b81e20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x564845b28120_0, 0, 2;
    %fork TD_tb_spi_adc.spi_read, S_0x564845b79cf0;
    %join;
    %delay 200000, 0;
    %load/vec4 v0x564845b81e20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
T_0.4 ;
    %load/vec4 v0x564845b81e20_0;
    %nor/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.5, 6;
    %wait E_0x564845b07190;
    %jmp T_0.4;
T_0.5 ;
T_0.2 ;
T_0.0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x564845b59270_0, 0, 2;
    %pushi/vec4 0, 0, 12;
    %store/vec4 v0x564845b7a200_0, 0, 12;
    %fork TD_tb_spi_adc.spi_write, S_0x564845b79f70;
    %join;
    %delay 5000000, 0;
    %end;
S_0x564845b42600 .scope task, "check" "check" 2 96, 2 96 0, S_0x564845b2e750;
 .timescale -9 -12;
v0x564845b46ca0_0 .var "actual", 11 0;
v0x564845b46d40_0 .var "expected", 11 0;
v0x564845b59310_0 .var "msg", 400 1;
TD_tb_spi_adc.check ;
    %load/vec4 v0x564845b46ca0_0;
    %load/vec4 v0x564845b46d40_0;
    %cmp/e;
    %jmp/1 T_1.8, 6;
    %flag_mov 8, 6;
    %load/vec4 v0x564845b46d40_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %load/vec4 v0x564845b46ca0_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %flag_get/vec4 5;
    %jmp/0 T_1.9, 5;
    %load/vec4 v0x564845b46ca0_0;
    %pad/u 32;
    %load/vec4 v0x564845b46d40_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
T_1.9;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %flag_mov 6, 9;
T_1.8;
    %jmp/0xz  T_1.6, 6;
    %vpi_call 2 99 "$display", "  [PASS] %s: 0x%h", v0x564845b59310_0, v0x564845b46ca0_0 {0 0 0};
    %jmp T_1.7;
T_1.6 ;
    %vpi_call 2 101 "$display", "  [FAIL] %s: Expected 0x%h, Got 0x%h", v0x564845b59310_0, v0x564845b46d40_0, v0x564845b46ca0_0 {0 0 0};
    %vpi_call 2 102 "$finish" {0 0 0};
T_1.7 ;
    %end;
