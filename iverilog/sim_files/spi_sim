    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %flag_get/vec4 5;
    %jmp/0 T_1.9, 5;
    %load/vec4 v0x580aa6ea14c0_0;
    %pad/u 32;
    %load/vec4 v0x580aa6ea1560_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
T_1.9;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %flag_mov 6, 9;
T_1.8;
    %jmp/0xz  T_1.6, 6;
