T_3.14 ; Top of for-loop
    %load/vec4 v0x5ee85abe3820_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_3.15, 5;
    %load/vec4 v0x5ee85abc1c60_0;
    %load/vec4 v0x5ee85abe3820_0;
    %part/s 1;
    %store/vec4 v0x5ee85ac08800_0, 0, 1;
