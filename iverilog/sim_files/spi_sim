    %join;
    %delay 200000, 0;
    %load/vec4 v0x5ee85ac086c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
T_0.4 ;
    %load/vec4 v0x5ee85ac086c0_0;
    %nor/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.5, 6;
    %wait E_0x5ee85ab90110;
    %jmp T_0.4;
T_0.5 ;
T_0.2 ;
T_0.0 ;
