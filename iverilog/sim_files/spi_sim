TD_tb_spi_adc.spi_read ;
    %pushi/vec4 0, 0, 2;
    %load/vec4 v0x564845b28120_0;
    %concat/vec4; draw_concat_vec4
    %concati/vec4 0, 0, 12;
    %store/vec4 v0x564845b39e80_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564845b81880_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x564845b5c160_0, 0, 32;
T_3.14 ; Top of for-loop
    %load/vec4 v0x564845b5c160_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_3.15, 5;
    %load/vec4 v0x564845b39e80_0;
    %load/vec4 v0x564845b5c160_0;
    %part/s 1;
    %store/vec4 v0x564845b81f60_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564845b82320_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564845b82320_0, 0, 1;
T_3.16 ; for-loop step statement
    %load/vec4 v0x564845b5c160_0;
    %subi 1, 0, 32;
    %store/vec4 v0x564845b5c160_0, 0, 32;
    %jmp T_3.14;
T_3.15 ; for-loop exit label
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564845b81880_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564845b81f60_0, 0, 1;
    %delay 200000, 0;
    %end;
