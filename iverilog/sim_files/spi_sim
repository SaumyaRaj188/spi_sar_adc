TD_tb_spi_adc.check ;
    %load/vec4 v0x5b8661a8eae0_0;
    %load/vec4 v0x5b8661a8eb80_0;
    %cmp/e;
    %jmp/1 T_1.8, 6;
    %flag_mov 8, 6;
    %load/vec4 v0x5b8661a8eb80_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %load/vec4 v0x5b8661a8eae0_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %flag_get/vec4 5;
    %jmp/0 T_1.9, 5;
    %load/vec4 v0x5b8661a8eae0_0;
    %pad/u 32;
    %load/vec4 v0x5b8661a8eb80_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
T_1.9;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %flag_mov 6, 9;
T_1.8;
    %jmp/0xz  T_1.6, 6;
    %vpi_call 2 99 "$display", "  [PASS] %s: 0x%h", v0x5b8661aa1150_0, v0x5b8661a8eae0_0 {0 0 0};
    %jmp T_1.7;
T_1.6 ;
    %vpi_call 2 101 "$display", "  [FAIL] %s: Expected 0x%h, Got 0x%h", v0x5b8661aa1150_0, v0x5b8661a8eb80_0, v0x5b8661a8eae0_0 {0 0 0};
    %vpi_call 2 102 "$finish" {0 0 0};
T_1.7 ;
    %end;
