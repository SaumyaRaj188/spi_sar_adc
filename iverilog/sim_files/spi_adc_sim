TD_tb_spi_adc.adc_hard_reset ;
    %load/vec4 v0x60b21d2e6d10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x60b21d28c150_0, 0, 2;
    %fork TD_tb_spi_adc.spi_read, S_0x60b21d2de9a0;
    %join;
    %delay 200000, 0;
    %load/vec4 v0x60b21d2e6d10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
T_0.4 ;
    %load/vec4 v0x60b21d2e6d10_0;
    %nor/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.5, 6;
    %wait E_0x60b21d26afd0;
    %jmp T_0.4;
T_0.5 ;
T_0.2 ;
T_0.0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x60b21d2be180_0, 0, 2;
    %pushi/vec4 0, 0, 12;
    %store/vec4 v0x60b21d2deeb0_0, 0, 12;
