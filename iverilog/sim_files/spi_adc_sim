S_0x60b21d2a74f0 .scope task, "check" "check" 2 96, 2 96 0, S_0x60b21d293300;
 .timescale -9 -12;
v0x60b21d2abb90_0 .var "actual", 11 0;
v0x60b21d2abc30_0 .var "expected", 11 0;
