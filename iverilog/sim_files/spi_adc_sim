    %store/vec4 v0x60b21d2c1070_0, 0, 32;
T_3.14 ; Top of for-loop
    %load/vec4 v0x60b21d2c1070_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
	  %jmp/0xz T_3.15, 5;
    %load/vec4 v0x60b21d29ea00_0;
    %load/vec4 v0x60b21d2c1070_0;
    %part/s 1;
    %store/vec4 v0x60b21d2e6e50_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60b21d2e7210_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60b21d2e7210_0, 0, 1;
T_3.16 ; for-loop step statement
